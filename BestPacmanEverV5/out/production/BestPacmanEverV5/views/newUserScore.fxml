                  <Font name="Avenir Next Heavy" size="22.0" />
               </font></TextField>
            <Button layoutX="479.0" layoutY="399.0" mnemonicParsing="false" onAction="#makeLeaderboard" prefHeight="27.0" prefWidth="282.0" style="-fx-background-color: #ffd900;" text="Submit Score">
