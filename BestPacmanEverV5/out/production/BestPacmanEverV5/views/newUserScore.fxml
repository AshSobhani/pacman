<GridPane alignment="center" hgap="10" vgap="10" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.UserScoreController">
   <children>
      <AnchorPane prefHeight="720" prefWidth="1225" style="-fx-background-color: black;">
         <children>
            <Label layoutX="261.0" layoutY="84.0" text="Join The LeaderBoard!" textFill="#ff3f30">
               <font>
                  <Font name="Avenir Next Heavy" size="63.0" />
               </font>
            </Label>
            <Label layoutX="258.0" layoutY="80.0" text="Join The LeaderBoard!" textFill="#30f0ff">
               <font>
                  <Font name="Avenir Next Heavy" size="63.0" />
               </font>
            </Label>
            <Label layoutX="259.0" layoutY="82.0" text="Join The LeaderBoard!" textFill="#ffd900">
               <font>
                  <Font name="Avenir Next Heavy" size="63.0" />
               </font>
            </Label>
            <Button layoutX="554.0" layoutY="585.0" mnemonicParsing="false" onAction="#backToMain" prefHeight="43.0" prefWidth="153.0" text="Main Menu">
               <font>
                  <Font name="Avenir Next Heavy" size="16.0" />
               </font>
               <cursor>
                  <Cursor fx:constant="HAND" />
               </cursor>
            </Button>
            <TextField fx:id="uName" alignment="CENTER" layoutX="472.0" layoutY="338.0" onKeyPressed="#getUsername" promptText="Username">
